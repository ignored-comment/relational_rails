<%= form_with url: "/foods/#{@food.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br>

  <%= form.label "expired?" %>
  <%= form.check_box :expired, {}, checked = "true", unchecked = "false" %><br>

  <%= form.label "total items available" %>
  <%= form.number_field :total_items_available, in: 1..99, step: 1%><br>
  
  <%= form.submit 'Update Food' %>
<% end %>
