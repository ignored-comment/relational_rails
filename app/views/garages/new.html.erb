<%= form_with url: "/garages", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br>

  <%= form.label "at capacity?" %>
  <%= form.check_box :at_capacity, {}, checked = "true", unchecked = "false" %><br>

  <%= form.label "max capacity in motorcycles" %>
  <%= form.number_field :max_capacity, in: 1..99, step: 1%><br>
  
  <%= form.submit 'Create Garage' %>
<% end %>